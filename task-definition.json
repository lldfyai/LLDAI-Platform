{
  "family": "lldfyTasksFamily",
  "containerDefinitions": [
    {
      "name": "lldai-container",
      "image": "816069149838.dkr.ecr.us-west-2.amazonaws.com/fastapi-app:latest",
      "cpu": 0,
      "portMappings": [
        {
          "name": "lldai-container-8000-tcp",
          "containerPort": 8000,
          "hostPort": 8000,
          "protocol": "tcp",
          "appProtocol": "http"
        }
      ],
      "essential": true,
      "environment": [
        {
          "name": "DOCKER_HOST",
          "value": "unix:///var/run/docker.sock"
        },
        {
          "name": "AWS_ACCOUNT_ID",
          "value": "${AWS_ACCOUNT_ID}"
        },
        {
          "name": "AWS_ACCESS_KEY_ID",
          "value": "${AWS_ACCESS_KEY_ID}"
        },
        {
          "name": "AWS_SECRET_ACCESS_KEY",
          "value": "${AWS_SECRET_ACCESS_KEY}"
        },
        {
          "name": "OAUTH2_CLIENT_SECRET",
          "value": "${OAUTH2_CLIENT_SECRET}"
        },
        {
          "name": "OAUTH2_CLIENT_ID",
          "value": "${OAUTH2_CLIENT_ID}"
        },
        {
          "name": "DB_PASSWORD",
          "value": "${DB_PASSWORD}"
        },
        {
          "name": "DB_USER",
          "value": "${DB_USER}"
        },
        {
          "name": "DB_NAME",
          "value": "${DB_NAME}"
        },
        {
          "name": "DB_HOST",
          "value": "${DB_HOST}"
        },
        {
          "name": "USER_POOL_ID",
          "value": "${USER_POOL_ID}"
        },
        {
          "name": "COGNITO_CLIENT_ID",
          "value": "${COGNITO_CLIENT_ID}"
        },
        {
          "name": "COGNITO_CLIENT_SECRET",
          "value": "${COGNITO_CLIENT_SECRET}"
        },
        {
          "name": "SQLALCHEMY_DB_HOST",
          "value": "${SQLALCHEMY_DB_HOST}"
        }
      ],
      "mountPoints": [
        {
          "sourceVolume": "docker-socket",
          "containerPath": "/var/run/docker.sock",
          "readOnly": false
        }
      ],
      "volumesFrom": [],
      "privileged": true,
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/lldfyTasksFamily",
          "mode": "non-blocking",
          "awslogs-create-group": "true",
          "max-buffer-size": "25m",
          "awslogs-region": "us-west-2",
          "awslogs-stream-prefix": "ecs"
        }
      },
      "systemControls": []
    }
  ],
  "taskRoleArn": "arn:aws:iam::816069149838:role/ecsTaskExecutionRole",
  "executionRoleArn": "arn:aws:iam::816069149838:role/ecsTaskExecutionRole",
  "networkMode": "awsvpc",
  "volumes": [
    {
      "name": "docker-socket",
      "host": {
        "sourcePath": "/var/run/docker.sock"
      }
    }
  ],
  "placementConstraints": [],
  "requiresCompatibilities": [
    "EC2"
  ],
  "cpu": "1900",
  "memory": "3500",
  "runtimePlatform": {
    "cpuArchitecture": "X86_64",
    "operatingSystemFamily": "LINUX"
  }
}